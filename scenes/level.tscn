[sub_resource type="Shader" id="Shader_uhfbm"]
code = "shader_type canvas_item;

uniform sampler2D SCREEN_TEXTURE : hint_screen_texture;
uniform sampler2D mask_texture : hint_default_black;

uniform vec2 sprite_screen_uv;
uniform float wave_speed = 1.0;
uniform float wave_thickness : hint_range(0.001, 0.5) = 0.1;
uniform float wave_frequency = 10.0;
uniform float wave_intensity = 0.05;
uniform float mask_strength : hint_range(0.0, 1.0) = 1.0;

void fragment() {
    vec2 resolution = vec2(textureSize(SCREEN_TEXTURE, 0));
    float aspect = resolution.x / resolution.y;
    vec2 uv = SCREEN_UV;

    vec2 center = sprite_screen_uv;
    if (center == vec2(-1.0)) {
        center = vec2(0.5);
    }

    vec2 dir = uv - center;
    dir.x *= aspect;
    float dist = length(dir);

    float wave_progress = mod(TIME * wave_speed, 2.0);


    float ripple_band = smoothstep(wave_progress - wave_thickness, wave_progress, dist) *
                        (1.0 - smoothstep(wave_progress, wave_progress + wave_thickness, dist));

    if (ripple_band > 0.001) {
        vec2 dir_norm = normalize(dir);
        float wave = sin(dist * wave_frequency - wave_progress * 10.0);
        float mask_value = texture(mask_texture, uv).r;
        float strength = wave * wave_intensity * ripple_band * mask_value * mask_strength;
        uv += dir_norm * strength;
    }

    COLOR = texture(SCREEN_TEXTURE, uv);
}
