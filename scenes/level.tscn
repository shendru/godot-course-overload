[sub_resource type="GDScript" id="GDScript_sc620"]
script/source = "extends CanvasLayer


func _ready() -> void:
	hide_everything()

func hide_everything() -> void:
	$RichTextLabel.hide()
	$RichTextLabel2.hide()
	$RichTextLabel3.hide()

func show_everything() -> void:
	$RichTextLabel.show()
	$RichTextLabel2.show()
	$RichTextLabel3.show()
	
func animate() -> void:
	$AnimationPlayer.play(\"overload\")
	


func _on_spawner_overload() -> void:
	show_everything()
	animate()
"

